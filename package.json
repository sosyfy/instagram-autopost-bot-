{
  "name": "express-template",
  "version": "0.0.0",
  "description": "An easy to use express template",
  "imports": {
    "#config": "./src/config.js",
    "#controllers/*": "./src/controllers/*.js",
    "#errors/*": "./src/errors/*.js",
    "#lib/*": "./src/lib/*.js",
    "#middlewares/*": "./src/middlewares/*.js",
    "#models/*": "./src/models/*.js",
    "#routes/*": "./src/routes/*.js",
    "#services/*": "./src/services/*.js",
    "#docs/*": "./src/docs/*.json"
  },
  "main": "main.js",
  "scripts": {
    "dev": "nodemon src/main.js",
    "start": "node src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sosyfy/instagram-autopost-bot-.git"
  },
  "keywords": [
    "instagram",
    "bot"
  ],
  "author": "Sospeter Karanja",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sosyfy/instagram-autopost-bot-.git"
  },
  "homepage": "https://github.com/sosyfy/instagram-autopost-bot-.git",
  "devDependencies": {
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "mongoose": "^6.7.3",
    "path": "^0.12.7"
  }
}
